generator client {
  provider = "prisma-client-js"
 
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Siswa {
  kode_siswa Int @id @default(autoincrement())
  nama_siswa String 
  alamat_siswa String  @db.Text
  tgl_siswa DateTime
  jurusan_siswa String
  createdAt     DateTime  @default(now())
  updatedAt      DateTime  @updatedAt @default(now())
}