// untuk install 
npm init - y
npm install express cors @prisma/client 
cors itu untuk cors 
@prisma/client itu untuk install client
// versi nyaaaaa
npm install express cors @prisma/client@6.17.0
npm install --save-dev prisma@6.17.0 nodemon


//
npm i --save-dev prisma nodemon
prisma itu untuk install prisma
nodemon untuk install nodemon

// opsional untuk proses devopemnent aja  yang ada di 
  "prisma:generate" : "prisma generate",
    "prisma:migrate" : "prisma migrate dev"

// akan otomatis membuat folder prisma yang akan muncul schema prisma
npx prisma init


//

model Category {
  id            Int  @id @default(autoincrement())
  name          String
  description   String?
}

String?  ? itu di kosongin boleh

// 
npx prisma migrate dev init 
untuk migrate prisma

